<h3>{{ heading_title }}</h3>
<div class="row">
 {% for latestcategory in latestcategorys %}
  <div class="product-layout col-lg-3 col-md-3 col-sm-6 col-xs-12">
    <div class="product-thumb transition">
      <div class="image"><a href="{{ latestcategory.href }}"><img src="{{ latestcategory.thumb }}" alt="{{ latestcategory.name }}" title="{{ latestcategory.name }}" class="img-responsive" /></a></div>
      <div class="caption">
        <h4><a href="{{ latestcategory.href }}">{{ latestcategory.name }}</a></h4>
      </div>
    </div>
  </div>
  {% endfor %}
</div>
